#include<stdio.h>
void main()
{
int i,n,k,evensum,oddsum;
printf("enter a number to find factors:");
scanf("%d",&k);
printf("enter a number upto which we want to find factors:");
scanf("%d",&n);
for(i=1;i<=n;i++)
{
printf("%d",i);
if(i%2==0)
{
evensum=evensum+i;
}
else
oddsome=oddsome+i;
}
printf("evensum=%d,oddsum=%d",evensum,oddsum);
}
